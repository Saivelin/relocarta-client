    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.4.7",
    "antd-mask-input": "^2.0.7",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "geojson": "^0.5.0",
    "mapbox-gl": "^2.14.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.45.1",
    "react-icons": "^4.8.0",
    "react-map-gl": "^7.0.23",
    "react-redux": "^8.0.7",
    "react-router-dom": "^6.11.1",
    "yup": "^1.2.0"
